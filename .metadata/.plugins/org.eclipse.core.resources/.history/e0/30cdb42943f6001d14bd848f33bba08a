package com.sourav;

import java.util.Scanner;

public class Questions {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		System.out.println(isPrime(n));
	}
	static boolean isPrime(int n) {
		if(n == 1||n%2!=0) {
			return true;
		}
		return false;
		
	}
	
}
